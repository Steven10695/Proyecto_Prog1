  <section class="contact-page">
    <div class="container">
      <div class="text-center">
           <h2>Registrarse</h2>
      </div>
      <div class="row contact-wrap">
        <div class="col-md-6 col-md-offset-3">
          <div id="errormessage"></div>
          <form action="" method="post" role="form" class="contactForm">
            <div class="form-group">
              <input type="text" name="name" class="form-control" id="name" placeholder="Nombre"  data-msg="Ingrese un nombre de al menos 4 caracteres" />
              <div class="validation"></div>
            </div>
            <div class="form-group">
              <input type="email" class="form-control" name="email" id="email" placeholder="Correo electrónico" data-rule="email" data-msg="Ingrese un email válido" />
              <div class="validation"></div>
            </div>
            <div class="form-group">
              <input type="password" class="form-control" name="contraseña" id="email" placeholder="Contraseña" data-rule="password" data-msg="Ingrese una contraseña" />
              <div class="validation"></div>
            </div>
            <div class="form-group">
              <input type="telephone" class="form-control" name="contraseña" id="telefono" placeholder="Teléfono" data-rule="telephone" data-msg="Ingrese su número de teléfono" />
              <div class="validation"></div>
            </div>
            <div class="form-group">
              <input type="adress" class="form-control" name="direccion" id="direccion" placeholder="Direccion" data-rule="adress" data-msg="Ingrese su distrito" />
              <div class="validation"></div>
            </div>
            <div class="text-center"><button type="submit" name="Enviar" class="btn btn-primary btn-lg" required="required">Registrarse</button></div>
          </form>
        </div>
      </div>
      <!--/.row-->
    </div>
    <!--/.container-->
  </section>
  <!--/#contact-page-->




</body>

</html>