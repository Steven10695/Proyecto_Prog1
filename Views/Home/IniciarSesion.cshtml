<div class="container">
  <section class="contact-page">
    <div class="container">
      <div class="text-center">
        <h2><b>INICIAR SESIÓN</b></h2>
      </div>
      <div class="row contact-wrap">
        <div class="col-md-6 col-md-offset-3">
          <div id="errormessage"></div>
          <form action="" method="post" role="form" class="contactForm" >
            <div class="form-group">
            <div class="form-group">

              <input type="email" class="form-control" name="email" id="email" placeholder="Correo electrónico" data-rule="email" data-msg="Ingrese un email válido" />
              <div class="validation"></div>
            </div>
            <input type="password" class="form-control" name="passsword" id="password" placeholder="Contraseña" data-rule="password" data-msg="Ingrese una contraseña válida" />
              <div class="validation"></div>
            </div>
            <div class="text-center"><button type="submit" name="Iniciar Sesion" class="btn btn-primary btn-lg" required="required">Iniciar Sesion</button></div>
            <br></br>
            <p>¿No tienes cuenta? <a href="Registrarse">Regístrate</a></p>
            <br></br>
          </form>
        </div>
      </div>
      <!--/.row-->
    </div>
    <!--/.container-->
  </section>
  <!--/#contact-page-->


 

  
  <script src="contactform/contactform.js"></script>

</body>

